<config>
  <project defaultFont='Arial'/>
  <root meta='{"maxColumns":5,"maxRows":5}' e='$add("NET")' />
  <additions>
  	<carousel name='NET' x='$stage.stageWidth/2' y='$stage.stageHeight/2' isHORIZONTAL='false' meta='{"addedToStageAction":[
   	{"type":"addTo","value":["row","NET","addToRail",null,"additions",true],"repeat":"$meta.maxRows"}],"gapswing":[1,{"GAP":20},null,0,true,"easeInOutQuint"]}' />
    <carousel name='row' isHORIZONTAL='true' meta='{"addedToStageAction":[
    	{"type":"addTo","value":["tfdef","row","addToRail",null,"additions",true],"repeat":"$meta.maxColumns"},
		{"type":"binCommand","value":"registry.row.name = \"row\" + String(registry.NET.numRailChildren)"},
		{"type":"animateAllRegisteredToScreen","value":"gapswing"}
		],"gapswing":[1,{"GAP":20},null,0,true,"easeInOutQuint"]}'/>
	<txt name='tfdef' width='40' height='40' background='true' border='true' backgroundColor='0xffffff' color='0' size='35' align='center' meta='{"addedToStageAction":[
		{"type":"binCommand","value":[["registry.tfdef.name = \"cell\" + String(registry.row.numRailChildren)","registry.NET.rearange(),registry.NET.movementBit(0.05)"]]}]}'>A</txt>
  </additions>
</config>